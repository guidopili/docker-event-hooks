{
  "version": "1",
  "options": {
    "compose-file-path": null,
    "filters": [
      {
        "type": "network",
        "action": "create",
        "identifier": "asd"
      },
      {
        "type": "container",
        "action": "create"
      }
    ]
  },
  "hooks": [
    {
      "on": [
        {
          "type": "network",
          "action": "create"
        },
        {
          "type": "container",
          "action": "create"
        }
      ],
      "command": [
        "curl",
        "-XPOST",
        "https://webhook.site/44772a00-60cb-44b5-8384-340f10a8ae89"
      ],
      "arguments": [
        "-d Time={{.TimeNano}}",
        "-d Actor={{index .Actor.Attributes \"name\"}}"
      ]
    }
  ]
}